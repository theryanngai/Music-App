<h1>Create a New Artist</h1>

<form action="<%= bands_url %>" method="post">

<input 
	name="authenticity_token"
	type="hidden"
	value="<%= form_authenticity_token %>">

<label for="band_name">Name</label>
	<input name="band[name]", id="band_name">
<br><br>

<input type="submit" value="Submit!">
</form>
<br><br>
<%= link_to "<< Back to all Bands", bands_url %>
